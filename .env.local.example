# PostgreSQL Configuration
# Database server IP/hostname (where PostgreSQL is running)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=layout_app
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_SSL=false

# API Configuration
# Application server IP (where the backend/API is running)
# Change localhost to your server IP if accessing from another machine
# Example: VITE_API_URL=http://192.168.70.90:3001/api
VITE_API_URL=http://localhost:3001/api

# JWT Secret (change in production)
VITE_JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
